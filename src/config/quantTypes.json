{
  "libraryPlateReader": {
    "key": "libraryPlateReader",
    "name": "Library - Plate Reader",
    "wellType": "PlateReader",
    "cvThreshold": 20,
    "parse": {
      "delimiter": ",",
      "rowDelimiter": ["\r\n", "\r", "\n", "\r\r\n"],
      "from": 16,
      "columns": [
        "row",
        "column",
        "content",
        "id",
        "concentration"
      ]
    },
    "metadata": {
      "rows": 3,
      "idColumn": "ID1",
      "delimiter": ","
    },
    "conversion": {
      "factors": {
        "averageLibrarySize": 573
      },
      "expression": "(1515.151*(1/averageLibrarySize))"
    },
    "qcResults": {
      "key": "molarity",
      "units": "nM",
      "assay": {
        "type": "Library - Plate Reader",
        "version": "v1.0"
      }
    }
  },
