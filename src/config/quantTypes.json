  "libraryPlateReader": {
    "key": "libraryPlateReader",
    "name": "Library - Plate Reader",
    "wellType": "PlateReader",
    "parse": {
      "delimiter": ",",
      "rowDelimiter": ["\r\n", "\r", "\n", "\r\r\n"],
      "from": 17,
      "columns": [
        "row",
        "column",
        "content",
        "id",
        "concentration"
      ]
    },
    "metadata": {
      "rows": 4,
      "idColumn": "ID1",
      "delimiter": ","
    },
    "conversion": {
      "factors": {
        "averageLibrarySize": 573
      },
      "expression": "(1515.151*(1/averageLibrarySize))",
      "decimalPlaces": 3
    },
    "qcResults": {
      "key": "molarity",
      "units": "nM",
      "assay": {
        "type": "Library - Plate Reader",
        "version": "v1.0"
      },
      "outlier": {
        "type": "cv",
        "threshold": 20
      },
      "fields": [
        "well_location",
        "key",
        "value",
        "units",
        "cv",
        "assay_type",
        "assay_version"
      ]
    }
  },
