  "libraryQPCR10ul": {
    "key": "libraryQPCR10ul",
    "name": "Library - qPCR - 10ul",
    "wellType": "QPCR10ul",
    "cvThreshold": 15,
    "parse": {
      "delimiter": "\t",
      "rowDelimiter": ["\r\n", "\r", "\n", "\r\r\n"],
      "from": 3,
      "relax_column_count": true,
      "columns": [
        "include",
        "color",
        "pos",
        "name",
        "cp",
        "concentration",
        "standard",
        "status"
      ]
    },
    "metadata": {
      "rows": 1,
      "idColumn": "Experiment",
      "delimiter": "  "
    },
    "conversion": {
      "factors": {
        "dilution": 500,
        "standardInsertSize": 452,
        "libraryInsertSize": 573
      },
      "expression": "(((dilution*standardInsertSize)/libraryInsertSize)/1000)"
    },
    "qcResults": {
      "key": "molarity",
      "units": "nM",
      "assay": {
        "type": "Library - qPCR",
        "version": "v1.0"
      }
    }
  },
  "libraryQPCR5ul": {
    "key": "libraryQPCR5ul",
    "name": "Library - qPCR - 5ul",
    "wellType": "QPCR5ul",
    "cvThreshold": 15,
    "parse": {
      "delimiter": ",",
      "rowDelimiter": ["\r\n", "\r", "\n", "\r\r\n"],
      "from": 2,
      "columns": [
        "well",
        "copyNumber",
        "replicateError",
        "totalError",
        "shapeZError",
        "Comments"
      ]
    },
    "metadata": {
    },
    "conversion": {
      "factors": {
        "topM": 3.321e-7,
        "calibration": 1.786,
        "dilution": 0.5
      },
      "expression": "(topM*calibration*dilution)"
    },
    "qcResults": {
      "key": "molarity",
      "units": "nM",
      "assay": {
        "type": "Library - qPCR",
        "version": "v2.0"
      }
    }
  }
