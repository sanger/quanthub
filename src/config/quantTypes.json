{
  "ISC": {
    "key": "ISC",
    "name": "ISC",
    "wellType": "PlateReader",
    "parse": {
      "delimiter": ",",
      "rowDelimiter": ["\r\n", "\r", "\n", "\r\r\n"],
      "from": 17,
      "columns": [
        "row",
        "column",
        "content",
        "id",
        "concentration"
      ]
    },
    "metadata": {
      "rows": 4,
      "idColumn": "ID1",
      "delimiter": ","
    },
    "qcResults": {
      "key": "concentration",
      "units": "ng/ul",
      "assay": {
        "type": "ISC",
        "version": "v1.0"
      },
      "outlier": {
        "type": "cv",
        "threshold": 15
      },
      "fields": [
        "well_location",
        "key",
        "value",
        "units",
        "cv",
        "assay_type",
        "assay_version"
      ]
    }
  },
