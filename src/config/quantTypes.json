  "libraryPlateReader": {
    "key": "libraryPlateReader",
    "name": "Library - Plate Reader",
    "wellType": "PlateReader",
    "parse": {
      "delimiter": ",",
      "rowDelimiter": ["\r\n", "\r", "\n", "\r\r\n"],
      "from": 17,
