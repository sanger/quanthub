  "libraryQPCR10ul": {
    "key": "libraryQPCR10ul",
    "name": "Library - qPCR - 10ul",
    "wellType": "QPCR10ul",
    "parse": {
      "delimiter": "\t",
      "rowDelimiter": ["\r\n", "\r", "\n", "\r\r\n"],
      "from": 3,
      "relax_column_count": true,
      "columns": [
        "include",
        "color",
        "pos",
        "name",
        "cp",
        "concentration",
        "standard",
        "status"
      ]
    },
    "metadata": {
      "rows": 1,
      "idColumn": "Experiment",
      "delimiter": "  "
    },
    "conversion": {
      "factors": {
        "dilution": 500,
        "standardInsertSize": 452,
        "libraryInsertSize": 573
      },
      "expression": "(ORIGINAL_VALUE * ((dilution*standardInsertSize)/libraryInsertSize)/1000)",
      "decimalPlaces": 3
    },
    "qcResults": {
      "key": "molarity",
      "units": "nM",
      "assay": {
        "type": "Library - qPCR",
        "version": "v1.0"
      },
      "outlier": {
        "type": "cv",
        "threshold": 15
      },
      "fields": [
        "barcode",
        "well_location",
        "key",
        "value",
        "units",
        "cv",
        "assay_type",
        "assay_version"
      ]
    }
  },
  "libraryQPCR5ul": {
    "key": "libraryQPCR5ul",
    "name": "Library - qPCR - 5ul",
    "wellType": "QPCR5ul",
    "parse": {
      "delimiter": ",",
